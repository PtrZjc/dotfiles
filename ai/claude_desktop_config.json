{
  "globalShortcut": "Shift+Ctrl+Space",
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/p.zajac/workspace",
        "/Users/p.zajac/Documents",
        "/Users/p.zajac/Library/CloudStorage/Dropbox/obsidian"
      ],
      "env": {}
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "jetbrains": {
      "command": "npx",
      "args": [
        "-y",
        "@jetbrains/mcp-proxy"
      ]
    }
  },
  "preferences": {
    "quickEntryShortcut": "off"
  }
}